<body id="body_login">
   <header><img src="logo.png" alt=""></header>


   <section class="section">
      <div class="formulario">
      <form action="/login" method="POST" class="box">
        
         <h1>LOGIN </h1>
         <h3>Por favor, identifique-se <br> (usuário e senha do moodle)</h3>
         <div id="user_senha">
         <label for="user">Identificação do Usuário: </label>
         <input type="text" name="user" placeholder="Identificação do usuário">

         <label for="senha">Senha: </label>
         <input type="password" name="senha" placeholder="Senha">
         </div>
         <button id="login" type="submit">ENTRAR</button>
         
      </form>
      </div>

   </section>


